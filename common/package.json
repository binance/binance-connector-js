{
  "name": "@binance/common",
  "description": "Binance Common Types and Utilities for Binance Connectors",
  "version": "2.3.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "npm run clean && npm run typecheck && tsdown",
    "typecheck": "tsc --noEmit",
    "clean": "shx rm -rf dist",
    "test": "jest --maxWorkers=50% --bail",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "lint": "eslint '**/*.ts' --fix",
    "security-check": "npm audit fix"
  },
  "keywords": [
    "Binance",
    "API",
    "Connector",
    "Crypto"
  ],
  "author": "Binance",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/binance/binance-connector-js.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.24",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.5",
    "tsdown": "^0.16.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "@types/ws": "^8.5.14",
    "axios": "^1.7.9",
    "json-with-bigint": "^3.4.4",
    "ws": "^8.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
