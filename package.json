{
  "name": "binance-connector",
  "version": "0.5.6",
  "description": "This is a lightweight library that works as a connector to the Binance public API.",
  "main": "src/index.js",
  "scripts": {
    "jsdoc": "jsdoc -c conf.json && ./docs_src/docs.sh",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "standard": "standard",
    "standard:fix": "standard --fix ",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binance/binance-connector.git"
  },
  "engines": {
    "node": ">=10.24.1",
    "npm": ">=6.14.12"
  },
  "keywords": [
    "Binance",
    "API"
  ],
  "author": "Binance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binance/binance-connector/issues"
  },
  "homepage": "https://github.com/binance/binance-connector#readme",
  "devDependencies": {
    "clean-jsdoc-theme": "^3.2.7",
    "husky": "^6.0.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.7",
    "marked": "^2.1.1",
    "nock": "^13.0.2",
    "standard": "^14.3.4"
  },
  "files": [
    "src/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "ws": "^7.5.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "standard": {
    "ignore": [
      "/docs/**/*.js"
    ]
  }
}
